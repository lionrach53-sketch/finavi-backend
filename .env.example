# Example environment variables for Coach Financier backend
# Copy this file to .env and fill values before starting the server

# NODE environment: development or production
NODE_ENV=production

# Server port
PORT=3000

# MongoDB connection string (production instance URI)
# e.g. mongodb+srv://user:pass@cluster0.mongodb.net/coach-financier?retryWrites=true&w=majority
MONGODB_URI=

# Optional: CORS origin for frontend (set to your frontend URL)
CORS_ORIGIN=

# Web-push VAPID keys (optional)
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# OpenAI API key (optional, required for AI endpoints)
OPENAI_API_KEY=

# Rate limiting configuration (milliseconds and max requests)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
# Copy this file to .env and fill the value. Do NOT commit your real key.
OPENAI_API_KEY=
PORT=3000
MONGODB_URI=
NODE_ENV=development
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
